<p id="notice"><%= notice %></p>

<h1>Listing Tamires</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tamires.each do |tamire| %>
      <tr>
        <td><%= link_to 'Show', tamire %></td>
        <td><%= link_to 'Edit', edit_tamire_path(tamire) %></td>
        <td><%= link_to 'Destroy', tamire, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tamire', new_tamire_path %>
